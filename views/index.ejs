<!--<h2>Map</h2>-->
<div class="row">
  <div class="col-md-12">
      <div id="map"></div>
  </div>
</div>
<div class="row">
  <div class="col-md-2">
    <div class ="logoSlogo">
     <img src="/img/logo.png" id = "logo" alt="hunkerup" height="33" width="50">
     <!--<p class="slogan" >Keeping you Safe and Dry!</p>-->
     </div>
     <div class="addForm">
     <h3 id="add">Report an incident</h3>

      <script>
        var markers = <%- JSON.stringify(places) %>;
      </script>

      <script defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCsOviKubGOIxhM8kkaClBBQtXg-Pk4wEM&callback=initMap">

      </script>

      <form action="/places" method="POST">

        <div class="form-group">
          <input type="text" class="form-control" name="name" placeholder="Name">
        </div>
        <div class="form-group">
          <input type="text" class="form-control" name="address" placeholder="Address">

        </div>
    
          <input type="submit" class="btn btn-primary">
      </form>
      </div>

  </div>
  <div class="col-md-4">
    <div class="places">
        <h3 id="places">Incidents Reported</h3>
      <% places.forEach(function(place) { %>
        <div class="well">
          <%= place.name %>
        </div>
        <% }); %>
    </div>
    
    
  </div>
  <div class="col-md-3">
   
  </div>
  <div class="col-md-3"></div>

</div>

<!-- scripts -->
<script src="/js/script.js"></script>


